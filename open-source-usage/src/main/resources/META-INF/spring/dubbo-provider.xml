<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="demo-provider" owner="afred" organization="afred"/>

    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>

    <!--uncomment this if you want to test dubbo's monitor-->
    <!--<dubbo:monitor protocol="registry"/>-->

    <!--<dubbo:provider id="demo" protocol="dubbo" />-->
    <dubbo:protocol name="dubbo"/>


    <dubbo:service interface="afred.javademo.dubbo.logfilter.HelloService" ref="helloService" filter="requestFilter"/>

    <bean id="helloService" class="afred.javademo.dubbo.logfilter.impl.TelphoneSayHello" />

</beans>